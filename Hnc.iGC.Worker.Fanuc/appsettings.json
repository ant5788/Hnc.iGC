{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "Hnc.iGC": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {
      "Application": "Hnc.iGC.Worker.Fanuc"
    }
  },
  "HnciGCApi": {
    "BaseAddress": "http://127.0.0.1:24912",
    "GetCNC": "/api/CNC",
    "PostCNC": "/api/CNC",
    "DeleteCNC": "/api/CNC",
    "GetBalancer": "/api/Balancer",
    "PostBalancer": "/api/Balancer",
    "DeleteBalancer": "/api/Balancer"
  },
  "DeviceSettings": {
    "Devices": [
      {
        "EnableDataCollect": true,
        "IntervalInMilliseconds": 1000,
        "DeviceId": "0659F64B-F030-4449-8AF8-5187D1E694FA",
        "Name": "MF(VirtualMachine)",
        "Description": "Fanuc 0i-MF Plus",
        "Protocal": "Focas1",
        "IP": "192.168.30.51",
        "Port": 8193
      }
    ]
  }
}